#include <stdio.h>
int main()
{
    int T,i,j,k;
    char s[10000];

    scanf("%d",&T);
    for(i=0;i<T;i++){
        scanf(" %[^\n]",s);
        k=0;
        for(j=0;j<strlen(s);j++){
                  if(s[j] == ' ' && s[j+1] != ' '){
                        k++;

            }

        }
        printf("Count = %d\n",k+1);

    }
    return 0;
}
