#include <stdio.h>
int main(){

    int T,i,n,j;
    scanf("%d",&T);



    while(T--){
           scanf("%d",&n);
          int ara[n];

        for(j = 0;j<n;j++){
            scanf("%d",&ara[j]);
            }
        ara[j]='\0';

       for(i = 0;i < n ;i=i+2){

            
                printf("%d ",ara[i]);
                


        }

            printf("\n");
    }


    return 0;
}
