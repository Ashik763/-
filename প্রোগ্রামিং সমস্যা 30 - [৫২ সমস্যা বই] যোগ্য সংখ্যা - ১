#include <stdio.h>

int main()
{
    int T,N,i,j;
    scanf("%d",&T);
    while(T--){
        scanf("%d",&N);
        int count = 0;
        int a=0;
        for(i=1;i<N;i++){
            if(N%i == 0){
                a=a+i;
            }

        }
    
        if(a == N){
            printf("YES, %d is a perfect number!\n",N);
        }
        else{
            printf("NO, %d is not a perfect number!\n",N);
        }
    }

    return 0;
}
