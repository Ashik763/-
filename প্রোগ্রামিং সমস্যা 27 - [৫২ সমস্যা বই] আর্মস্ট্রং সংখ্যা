#include<stdio.h>

int main(){

    int n,a,b,c,d,T,e;
    scanf("%d",&T);
    while(T--){

    scanf("%d",&n);
    e=n;
    c=0;
    b=0;

    while(n != 0){

            a=n%10;

            n=n/10;
           
            b=a*a*a;
            c=c+b;


    }
        if(e==c){
                     printf("%d is an armstrong number!\n",e);
                }
        else{
            printf("%d is not an armstrong number!\n",e);
         }

}



    return 0;
}
